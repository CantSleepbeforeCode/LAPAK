<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dsc.lapak">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/noActionBar">
        <activity android:name=".activity.BikeActivity"
            android:theme="@style/AppTheme"></activity>
        <activity android:name=".activity.vehicle.AddVehicleActivity"
            android:theme="@style/AppTheme"/>
        <activity
            android:name=".activity.vehicle.MyVehicleActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".activity.ParkingAreaActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".activity.QRPaymentActivity"
            android:theme="@style/AppTheme" />
        <activity android:name=".activity.SignUpActivity" />
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".activity.HomeActivity" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>